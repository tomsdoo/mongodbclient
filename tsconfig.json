{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["esnext"],
    "declaration": true,
    "strict": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
