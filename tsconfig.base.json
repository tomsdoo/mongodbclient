{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": ["esnext"],
    "declaration": true,
     "outDir": "./dist/",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
